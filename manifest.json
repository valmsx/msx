"actions": {
  "youtube:search": {
    "type": "keyboard",
    "title": "Cerca su YouTube",
    "action": "content:load",
    "payload": {
      "type": "fetch",
      "url": "/msx_search?input={input}"
    }
  },
  "youtube:play": {
    "type": "video",
    "player": "youtube",
    "source": "payload.videoId"
  }
}
